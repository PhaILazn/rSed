  <body>
    <%- include("../partials/header.ejs") %>

    <h1 align="center" style="padding-top: 75px; ">Mexican</h1>

    <div id="container" class="cols" align="center">
      <div class="box">
        <a href="restaurantprofile/5e7d983279fdd01d28baeaf6" style="text-decoration: none;">
          <img
            src="https://photos.bringfido.com/restaurants/temp/131e.jpg?size=stack&density=2x"
